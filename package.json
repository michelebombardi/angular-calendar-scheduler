{
  "name": "angular-calendar-scheduler",
  "version": "1.9.1",
  "license": "MIT",
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist",
    "build:package": "ng-packagr -p ng-package.json",
    "build:dist": "npm run build:package && npm run build:styles && npm run copyfiles",
    "build:styles": "node-sass lib/modules/scheduler/calendar-scheduler-view.component.scss | postcss --output dist/css/angular-calendar-scheduler.css",
    "copyfiles": "copyfiles CHANGELOG.md dist && copyfiles -u 1 lib/modules/**/package.json dist && copyfiles -u 1 lib/**/*.scss dist/scss",
    "release:git": "git add . package.json yarn.lock && git commit -m \"chore: bump version number\" && standard-version --release-as patch && git push --follow-tags origin master",
    "release:npm": "npm run build:dist && npm publish dist",
    "release": "npm run release:git && npm run release:npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mounthorse-slns/angular-calendar-scheduler.git"
  },
