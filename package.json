    "date-fns": "^2.8.1",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "^3.5.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
